var input = prompt("FizzBuzz");
>>>>> for( var n = 1; n<=input ;n++)
>>>>> {
>>>>> if(n%3===0&&n%5===0)
>>>>> {
>>>>> document.write("FizzBuzz");
>>>>> }
>>>>> else if(n%3===0)
>>>>> {
>>>>> document.write("Fizz");
>>>>> }
>>>>> else if(n%5===0)
>>>>> {
>>>>> document.write("Buzz");
>>>>> }
>>>>> else
>>>>> {
>>>>> document.write(n);
>>>>> }
>>>>> }
